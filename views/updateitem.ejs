  <body class="text-center">
    <div class="container1">
      <form class="add-item" action="/updateitem" method="post">
        <img class="img-adjust" src="/favicons/news-icon.webp" alt="item-image" width="200" height="200">
        <h1 class="h3 mb-3 font-weight-normal brand">UPDATE MEDIA</h1>
        <label for="inputName" class="sr-only">Media Name</label>
        <input type="text" name="mediaName" id="inputName" class="form-control" placeholder="Media Name" required autofocus>
        <label for="inputImageURL" class="sr-only">Popularity</label>
        <input type="text" name="alexaRank" id="inputImageURL" class="form-control" placeholder="Alexa Rank" required>
        <label for="inputItemPrice" class="sr-only">Peer Reputation</label>
        <input type="text" name="pageRank" id="inputItemPrice" class="form-control" placeholder="Page Rank" required>
        <label for="inputNumberItems" class="sr-only">Media Bias</label>
        <input type="text" name="politicalBias" id="inputNumberItems" class="form-control" placeholder="Media Bias" required>
        <label for="inputfactCheck" class="sr-only">Fact Check</label>
        <input type="text" name="factCheck" id="inputfactCheck" class="form-control" placeholder="Fact Check" required>
        <button class="btn btn-lg btn-danger btn-block" type="submit">UPDATE MEDIA</button>
        <p class="mt-5 mb-3 text-muted">&copy; 2020-2021</p>
      </form>
    </div>
